<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CTAButton">
        <attr name="size" format="enum">
            <enum name="small" value="0" />
            <enum name="medium" value="1" />
            <enum name="large" value="2" />
        </attr>
        <attr name="buttonType" format="enum">
            <enum name="positive" value="0" />
            <enum name="warning" value="1" />
            <enum name="error" value="2" />
            <enum name="neutral" value="3" />
        </attr>
    </declare-styleable>
</resources>